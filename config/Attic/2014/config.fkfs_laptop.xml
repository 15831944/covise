   <!--MonoCommand value="echo mono > /proc/driver/genlock/state" /-->
   <ViewerPosition x="0" y="-1300" z="-220" />
   <FloorHeight value="-1250" />
   <StepSize value="400" />

   <Input>
    <ButtonSystem value="NONE # IAO" />
    <MouseNav value="ON" />
